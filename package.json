{
  "name": "leopard-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "concurrently \"npm run dev\" \"npm run proxy\"",
    "build": "vite build",
    "preview": "vite preview",
    "proxy": "nodemon --config nodemon.json proxy-server.cjs",
    "proxy:stop": "pkill -f 'node proxy-server.cjs'",
    "proxy:debug": "nodemon --config nodemon.json --inspect proxy-server.cjs"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.4",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.8",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5"
  }
}
