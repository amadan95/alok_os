{
  "name": "leopard-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "kill-port 3001 && kill-port 5173 && concurrently \"npm:dev\" \"npm:proxy\"",
    "build": "vite build && cp -r public/Calculette.png public/\"Film Cannister from Photoroom.png\" public/\"Microsoft Paint PNG.png\" public/\"Photoroom VHS.png\" public/QuickTime.png public/\"Safari Icon from Photoroom.png\" public/Safari.png public/TextEdit.png public/iPhoto.png public/\"iPod Image from Photoroom.png\" public/iTunes.png public/ipod.png dist/",
    "preview": "vite preview",
    "proxy": "nodemon --config nodemon.json proxy-server.cjs",
    "proxy:stop": "pkill -f 'node proxy-server.cjs'",
    "proxy:debug": "nodemon --config nodemon.json --inspect proxy-server.cjs",
    "vercel-build": "npm run build"
  },
  "devDependencies": {
    "@vercel/static-build": "^1.3.38",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8",
    "vite": "^5.4.2"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.8",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "harmon": "^1.3.2",
    "http-proxy-middleware": "^3.0.0",
    "npm-run-all": "^4.1.5"
  }
}
